#include <stdio.h>
#include <stdlib.h>

#define MAX_SIZE 100

struct Point{

	char *user[MAX_SIZE];
	char *pass[MAX_SIZE];

};

int main(void){

	struct Point group_points[MAX_SIZE];

	for (int i = 0; i < 5; ++i)
	{
		scanf("Enter a value for user: %s", group_points.user);
	}

	for (int i = 0; i < 5; ++i)
	{
		printf("ID#%d\n User value:%s\n", i, group_points.user);
	}


	return EXIT_SUCCESS;
}